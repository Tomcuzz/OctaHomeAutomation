{% load my_filters %}
<CiscoIPPhoneMenu>
	<Title>Home Automation System</Title>
	<Prompt>Please Select A Room</Prompt>
	{% for light in lights %}
		<MenuItem>
			<Name>{{ light.Name }}</Name>
			<URL>http://192.168.1.101{% url 'Lights' protocal='cisco' house=light.Room.Home.id room=light.Room.id lightType=light.getObjectType|last lightId=light.id %}</URL>
		</MenuItem>
	{% endfor %}
</CiscoIPPhoneMenu>